#!/bin/sh
current_dir=$(pwd)
script_dir=$(dirname $0)

if [ $script_dir = '.' ]
then
script_dir="$current_dir"
fi
cd $script_dir

#java -jar siddhi-launcher-1.0.0-SNAPSHOT.jar "$@"
java -classpath siddhi-launcher-1.0.0-SNAPSHOT.jar:../lib/* org.wso2.siddhi.runner.Main "$@"
